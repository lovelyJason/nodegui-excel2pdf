{
  "name": "nodegui-excel2pdf",
  "version": "1.0.0",
  "description": "A NodeGui project",
  "scripts": {
    "build": "npm run build-code && npm run build-bundle",
    "build-code": "tsc",
    "build-bundle": "esbuild build/main.js --sourcemap --bundle --outfile=dist/main.cjs --platform=node --format=cjs --external:@nodegui/nodegui \"--external:nodegui-plugin-*\"",
    "clean": "rimraf build dist tmp-jam-pack-nodegui",
    "package": "jam-pack-nodegui --config packaging/jam-pack-nodegui.json",
    "run": "qode dist/main.cjs",
    "start": "npm run build && npm run run",
    "start:watch": "nodemon --watch src -e js,jsx,ts,tsx --ignore dist/ --ignore node_modules/ --exec npm start"
  },
  "keywords": [
    "nodegui"
  ],
  "author": "JasonHuang",
  "license": "MIT",
  "devDependencies": {
    "@nodegui/packer": "^1.5.0",
    "@types/node": "^20.3.1",
    "esbuild": "^0.18.4",
    "jam-pack-nodegui": "^0.18.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@nodegui/nodegui": "^0.61.0",
    "docxtemplater": "^3.47.4",
    "exceljs": "^4.4.0",
    "node-xlsx": "^0.24.0",
    "nodemon": "^3.1.3",
    "pizzip": "^3.1.7",
    "source-map-support": "^0.5.21"
  }
}
